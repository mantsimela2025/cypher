# CYPHER Client Environment Configuration Template
# Copy this file to .env and update with your values

# =============================================================================
# API CONFIGURATION
# =============================================================================
# API Base URL - automatically determined based on environment
REACT_APP_API_BASE_URL=http://localhost:3001/api/v1

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
REACT_APP_NAME=CYPHER
REACT_APP_VERSION=1.0.0

# =============================================================================
# DEVELOPMENT OPTIONS
# =============================================================================
# Enable development features
REACT_APP_ENABLE_DEV_TOOLS=true
REACT_APP_ENABLE_CONSOLE_LOGS=true
REACT_APP_ENABLE_MOCK_DATA=false

# =============================================================================
# ENVIRONMENT-SPECIFIC NOTES
# =============================================================================
# Development: Use localhost:3001 for API
# Production: Use your actual API domain
# All REACT_APP_ variables are embedded in build and visible to users
