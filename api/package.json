{
  "name": "ras-dash-api",
  "version": "1.0.0",
  "description": "Backend API for RAS Dashboard application",
  "main": "server.js",
  "scripts": {
    "swagger:coverage": "node scripts/check-swagger-coverage.js",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:fast": "set DEFER_SERVICE_INIT=true&& set STARTUP_TIMING=true&& nodemon server.js",
    "dev:ultra": "set SKIP_HEAVY_SERVICES=true&& nodemon server.js",
    "dev:safe": "node scripts/start-api-safe.js",
    "dev:safe:force": "node scripts/start-api-safe.js --force",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api:distribution-groups": "cross-env NODE_ENV=test node API_TEST_SCRIPTS/test_distribution_groups_api.js",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:query": "node scripts/db-query.js",
    "db:seed": "node scripts/db-seed.js",
    "db:remove-duplicates": "node scripts/remove-duplicates.js",
    "test:email": "node scripts/test-email.js",
    "test:tenable:mock": "python testing/tenable/mock_tenable_server.py",
    "test:tenable:python": "python testing/tenable/test_pytenable_local.py",
    "test:tenable:nodejs": "npx tsx testing/tenable/test_tenable_nodejs.ts",
    "test:tenable:integration": "node testing/tenable/test_integration.js",
    "test:tenable:integration:changes": "node testing/tenable/test_integration.js --test-changes",
    "test:tenable:changes": "python testing/tenable/test_scheduled_changes.py",
    "test:xacta:integration": "node testing/tenable/test_xacta_integration.js",
    "test:nvd:integration": "node testing/tenable/test_nvd_integration.js",
    "import:nvd": "node scripts/import_nvd_data.js",
    "import:nvd:force": "node scripts/import_nvd_data.js --force",
    "import:nvd:recent": "node scripts/import_nvd_data.js --days=7 --max=500",
    "import:nvd:enhanced": "node scripts/import_enhanced_nvd.js",
    "check:cve": "node scripts/check_cve_data.js",
    "check:remediation": "node scripts/check_remediation_coverage.js",
    "debug:nvd": "node scripts/debug_nvd_response.js",
    "test:cve-api": "node scripts/test_cve_api.js",
    "test:asset-schemas": "node scripts/test_asset_schemas.js",
    "test:asset-management-schemas": "node scripts/test_asset_management_schemas.js",
    "test:nl-query-schemas": "node scripts/test_nl_query_schemas.js",
    "test:ato-schemas": "node scripts/test_ato_schemas.js",
    "test:audit-logs-schemas": "node scripts/test_audit_logs_schemas.js",
    "test:ato-api": "node scripts/test_ato_api.js",
    "test:audit-logs-api": "node scripts/test_audit_logs_api.js",
    "generate:asset-management-migration": "node scripts/create_asset_management_migration.js",
    "generate:nl-query-migration": "node scripts/create_nl_query_migration.js",
    "generate:ato-migration": "node scripts/create_ato_migration.js",
    "generate:audit-logs-migration": "node scripts/create_audit_logs_migration.js",
    "test:asset-management-api": "node scripts/test_asset_management_api.js",
    "test:asset-management-comprehensive": "node scripts/comprehensive_asset_management_api_test.js",
    "test:core-asset-crud": "node scripts/test_core_asset_crud.js",
    "test:asset-analytics-api": "node scripts/test_asset_analytics_api.js",
    "test:ai-cost-optimization-api": "node scripts/test_ai_cost_optimization_api.js",
    "test:nl-query-api": "node scripts/test_nl_query_api.js",
    "demo:tenable-xacta": "python testing/tenable/demo_tenable_xacta_integration.py",
    "test:tenable:setup": "cd testing/tenable && chmod +x setup.sh && ./setup.sh",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "nodejs",
    "express",
    "api",
    "postgresql",
    "drizzle",
    "jwt",
    "authentication"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.883.0",
    "@aws-sdk/client-ses": "^3.864.0",
    "@mermaid-js/mermaid-cli": "^11.9.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "openai": "^4.104.0",
    "postgres": "^3.4.3",
    "response-time": "^2.3.4",
    "sharp": "^0.34.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "axios": "^1.10.0",
    "drizzle-kit": "^0.20.4",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "tsx": "^4.20.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/db/migrations/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
